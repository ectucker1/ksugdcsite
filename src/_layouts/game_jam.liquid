---
layout: default
---
<br>
<div class="container text-center">
    <h1>K-State Annual Game Jam</h1>
    <div class="row">
        <div class="btn-container col">
            {%- for jam in collections.jam %}
                {% if page.url == jam.url %}
                    <a class="btn btn-secondary disabled">{{ jam.data.year }}</a>
                {% else %}
                    <a class="btn btn-success" href="{{ jam.url }}">{{ jam.data.year }}</a>
                {% endif %}
            {% endfor -%}
        </div>
    </div>

    {{ content }}
</div>
<br>
